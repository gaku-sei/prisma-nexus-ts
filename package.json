{
  "name": "prisma-ts",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "prettier": "prettier --check src",
    "prettier:fix": "prettier --write src",
    "ts:check": "tsc --noEmit",
    "ts:check:watch": "tsc --noEmit --watch",
    "nexus:dev": "ts-node-dev --respawn --poll --transpile-only --no-notify --watch graphql,src ./src/index.ts"
  },
  "devDependencies": {
    "@prisma/cli": "2.14.0",
    "@types/node": "^14.14.22",
    "prettier": "2.2.1",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@prisma/client": "^2.14.0",
    "apollo-server": "^2.19.2",
    "graphql": "^15.4.0",
    "nexus": "^1.0.0"
  }
}
